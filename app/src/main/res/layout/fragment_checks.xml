<?xml version="1.0" encoding="utf-8"?>
<com.sothree.slidinguppanel.SlidingUpPanelLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:sothree="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/panelChecks"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent"
    android:gravity="bottom"
    sothree:layout_anchorGravity="bottom"
    sothree:umanoAnchorPoint="0.68"
    sothree:umanoFadeColor="@android:color/transparent"
    sothree:umanoInitialState="hidden"
    sothree:umanoOverlay="true"
    sothree:umanoPanelHeight="0dp"
    sothree:umanoParallaxOffset="0dp"
    sothree:umanoShadowHeight="0dp"
    tools:context="com.djavid.checksonline.features.root.RootActivity"
    tools:ignore="MissingPrefix, ContentDescription">

    <include layout="@layout/fragment_checks_content" />

    <include layout="@layout/fragment_checks_panel" />

</com.sothree.slidinguppanel.SlidingUpPanelLayout>
